<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"        "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="cn.hm1006.boot.mapper.AirMapper">    <select id="findAll" resultType="cn.hm1006.boot.entity.Air">        select * from air    </select>    <select id="findbyId" resultType="cn.hm1006.boot.entity.Air">        select *        from air        WHERE id = #{id}    </select></mapper>